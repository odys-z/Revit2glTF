<?xml version="1.0" encoding="utf-8"?>
<package >
  <metadata>
    <id>GltfExport.RevitPlugin</id>
    <version>0.1.9</version>
    <title>Gltf Exporter for Revit</title>
    <authors>McCulloughRT, Odys Zhou</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <projectUrl>http://github.com/odys-z/Revit2glTF/</projectUrl>
    <icon>assets/odysz.png</icon>
    <description>A Revit 2017 plugin for exporting gltf and controlling rendering on x-visual.
